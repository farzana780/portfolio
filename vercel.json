{

  "builds": [
    {
      "src": "portfolio_project/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "15mb", "runtime": "python3.12.3"}
    }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "portfolio_project/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}